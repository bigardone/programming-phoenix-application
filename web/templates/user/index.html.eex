<h1>Listing Users</h1>

<table class="table">
  <tbody>
    <%= for user <- @users do %>
      <tr>
        <td>
          <strong><%= first_name(user) %> (<%= user.id %>)</strong>
        </td>
        <td>
          <%= link "View", to: user_path(@conn, :show, user.id) %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
